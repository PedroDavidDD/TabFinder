{
  "manifest_version": 3,
  "name": "Scrolling between pages",
  "version": "1.0",
  "description": "Descripción de mi extensión",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "tabs"],
  "background": {
    "service_worker": "background.js"
  }
}
